e":[7,7],"drawn":false,"documentCollection":"","documentId":null,"_id":"RK3VINFxJTeHciaA"},{"flags":{},"type":0,"text":"<b>[[+ 10]] ([[/r + 10]])</b>8\n\n<b>Damage Type</b>Psychic\n\n<b>Gem</b>Jade","img":"icons/svg/d20-black.svg","weight":1,"range":[8,8],"drawn":false,"documentCollection":"","documentId":null,"_id":"KhYkSi34BqE6lTaI"},{"flags":{},"type":0,"text":"<b>[[+ 10]] ([[/r + 10]])</b>9\n\n<b>Damage Type</b>Radiant\n\n<b>Gem</b>Topaz","img":"icons/svg/d20-black.svg","weight":1,"range":[9,9],"drawn":false,"documentCollection":"","documentId":null,"_id":"keJxXkcwRdeHAROI"},{"flags":{},"type":0,"text":"<b>[[+ 10]] ([[/r + 10]])</b>10\n\n<b>Damage Type</b>Thunder\n\n<b>Gem</b>Spinel","img":"icons/svg/d20-black.svg","weight":1,"range":[10,10],"drawn":false,"documentCollection":"","documentId":null,"_id":"hpuAyghJTY11eab2"}],"formula":"d10","replacement":true,"displayRoll":true,"_stats":{"systemId":"dnd5e","systemVersion":"2.0.2","coreVersion":"10.284","createdTime":1662395624302,"modifiedTime":1662395624302,"lastModifiedBy":"aGoxb8mMNhC874cZ"},"folder":null,"sort":0,"ownership":{"default":0,"aGoxb8mMNhC874cZ":3},"_id":"XbUBOg3nubMst4Vu"}
{"name":"Background: City Watch / Investigator: Flaw","flags":{"ddbimporter":{"parentName":"Background: City Watch / Investigator","keys":["d6","Flaw"],"diceKeys":["d6"]}},"img":"icons/svg/d20-grey.svg","description":"","results":[{"flags":{},"type":0,"text":"The monstrous enemy we faced in battle still leaves me quivering with fear.","img":"icons/svg/d20-black.svg","weight":1,"range":[1,1],"drawn":false,"documentCollection":"","documentId":null,"_id":"k5OiSF2XkzyXgFcu"},{"flags":{},"type":0,"text":"I have little respect for anyone who is not a proven warrior.","img":"icons/svg/d20-black.svg","weight":1,"range":[2,2],"drawn":false,"documentCollection":"","documentId":null,"_id":"T5HQlvRamowWp1QE"},{"flags":{},"type":0,"text":"I made a terrible mistake in battle that cost many lives, and I would do anything to keep that mistake secret.","img":"icons/svg/d20-black.svg","weight":1,"range":[3,3],"drawn":false,"documentCollection":"","documentId":null,"_id":"Nv1AAlqobgcewgCJ"},{"flags":{},"type":0,"text":"My hatred of my enemies is blind and unreasoning.","img":"icons/svg/d20-black.svg","weight":1,"range":[4,4],"drawn":false,"documentCollection":"","documentId":null,"_id":"NYGQikwWw52cdL4A"},{"flags":{},"type":0,"text":"I obey the law, even if the law causes misery.","img":"icons/svg/d20-black.svg","weight":1,"range":[5,5],"drawn":false,"documentCollection":"","documentId":null,"_id":"MmF7dnuzolJ5Tj2y"},{"flags":{},"type":0,"text":"I’d rather eat my armor than admit when I’m wrong.","img":"icons/svg/d20-black.svg","weight":1,"range":[6,6],"drawn":false,"documentCollection":"","documentId":null,"_id":"hQbFRodNXubXCWWw"}],"formula":"d6","replacement":true,"displayRoll":true,"_stats":{"systemId":"dnd5e","systemVersion":"2.0.2","coreVersion":"10.284","createdTime":1662396004130,"modifiedTime":1662396004130,"lastModifiedBy":"aGoxb8mMNhC874cZ"},"folder":null,"sort":0,"ownership":{"default":0,"aGoxb8mMNhC874cZ":3},"_id":"Y13GYTx64HL3ak2x"}
{"name":"Wild Magic Surge: Wild Magic Surge","flags":{"ddbimporter":{"parentName":"Wild Magic Surge","keys":["\nd100\n","\nEffect\n"],"diceKeys":["\nd100\n"]}},"img":"icons/svg/d20-grey.svg","description":"","results":[{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">Roll on this table at the start of each of your turns for the next minute, ignoring this result on subsequent rolls.</p>","img":"icons/svg/d20-black.svg","weight":1,"range":[1,2],"drawn":false,"documentCollection":"","documentId":null,"_id":"8zQSZC73zQSt02fx"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">For the next minute, you can see any invisible creature if you have line of sight to it.</p>","img":"icons/svg/d20-black.svg","weight":1,"range":[3,4],"drawn":false,"documentCollection":"","documentId":null,"_id":"g2SsYq0WmTi4zc9z"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">A modron chosen and controlled by the DM appears in an unoccupied space within 5 feet of you, then disappears 1 minute later.</p>","img":"icons/svg/d20-black.svg","weight":1,"range":[5,6],"drawn":false,"documentCollection":"","documentId":null,"_id":"XU16j9KdisYZeFGN"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">You cast <span class=\"Sans-Serif-Character-Styles_Italic-Sans-Serif\">fireball</span> as a 3rd-level spell centered on yourself.</p>","img":"icons/svg/d20-black.svg","weight":1,"range":[7,8],"drawn":false,"documentCollection":"","documentId":null,"_id":"6K88DlhM1xJgSmRI"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">You cast <span class=\"Sans-Serif-Character-Styles_Italic-Sans-Serif\">magic missile</span> as a 5th-level spell.</p>","img":"icons/svg/d20-black.svg","weight":1,"range":[9,10],"drawn":false,"documentCollection":"","documentId":null,"_id":"iMiVd2LWBjGFeP8W"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">Roll a[[+ 10]] ([[/r + 10]]). Yourheight changes by a number of inches equal to the roll. If the roll is odd, you shrink. If the roll is even, you grow.</p>","img":"icons/svg/d20-black.svg","weight":1,"range":[11,12],"drawn":false,"documentCollection":"","documentId":null,"_id":"DxRJStsFSRlIxZrl"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">You cast <span class=\"Sans-Serif-Character-Styles_Italic-Sans-Serif\">confusion</span> centered on yourself.</p>","img":"icons/svg/d20-black.svg","weight":1,"range":[13,14],"drawn":false,"documentCollection":"","documentId":null,"_id":"jpx3nNlZHN74GgcD"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">For the next minute, you regain 5 hit points at the start of each of your turns.</p>","img":"icons/svg/d20-black.svg","weight":1,"range":[15,16],"drawn":false,"documentCollection":"","documentId":null,"_id":"M8pMHCcKheeJ4flD"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">You grow a long beard made of feathers that remains until you sneeze, at which point the feathers explode out from your face.</p>","img":"icons/svg/d20-black.svg","weight":1,"range":[17,18],"drawn":false,"documentCollection":"","documentId":null,"_id":"P5YGQB68G0x9JQu2"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">You cast <span class=\"Sans-Serif-Character-Styles_Italic-Sans-Serif\">grease</span> centered on yourself.</p>","img":"icons/svg/d20-black.svg","weight":1,"range":[19,20],"drawn":false,"documentCollection":"","documentId":null,"_id":"jTgohEn3saudGoyT"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">Creatures have disadvantage on saving throws against the next spell you cast in the next minute that involves a saving throw.</p>","img":"icons/svg/d20-black.svg","weight":1,"range":[21,22],"drawn":false,"documentCollection":"","documentId":null,"_id":"9pfS67e4HpJykZ4p"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">Your skin turns a vibrant shade of blue. A <span class=\"Sans-Serif-Character-Styles_Italic-Sans-Serif\">remove curse</span> spell can end this effect.</p>","img":"icons/svg/d20-black.svg","weight":1,"range":[23,24],"drawn":false,"documentCollection":"","documentId":null,"_id":"DI76Wn8qZdbs2Txa"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">An eye appears on your forehead for the next minute. During that time, you have advantage on Wisdom (Perception) checks that rely on sight.</p>","img":"icons/svg/d20-black.svg","weight":1,"range":[25,26],"drawn":false,"documentCollection":"","documentId":null,"_id":"VJuwc6QTBggeFjPp"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">For the next minute, all your spells with a casting time of 1 action have a casting time of 1 bonus action.</p>","img":"icons/svg/d20-black.svg","weight":1,"range":[27,28],"drawn":false,"documentCollection":"","documentId":null,"_id":"azrSX6bX4FGUuocr"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">You teleport up to 60 feet to an unoccupied space of your choice that you can see.</p>","img":"icons/svg/d20-black.svg","weight":1,"range":[29,30],"drawn":false,"documentCollection":"","documentId":null,"_id":"8YyVo4Ssf66SAitM"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">You are transported to the Astral Plane until the end of your next turn, after which time you return to the space you previously occupied or the nearest unoccupied space if that space is occupied.</p>","img":"icons/svg/d20-black.svg","weight":1,"range":[31,32],"drawn":false,"documentCollection":"","documentId":null,"_id":"t4EHSL3EVyKiZle5"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">Maximize the damage of the next damaging spell you cast within the next minute.</p>","img":"icons/svg/d20-black.svg","weight":1,"range":[33,34],"drawn":false,"documentCollection":"","documentId":null,"_id":"J7QrzdfrGTzdyRMO"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">Roll a[[+ 10]] ([[/r + 10]]). Yourage changes by a number of years equal to the roll. If the roll is odd, you get younger (minimum 1 year old). If the roll is even, you get older.</p>","img":"icons/svg/d20-black.svg","weight":1,"range":[35,36],"drawn":false,"documentCollection":"","documentId":null,"_id":"91chsWiPrL3L61K9"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">[[1d6]] ([[/r 1d6]])  flumphscontrolled by the DM appear in unoccupied spaces within 60 feet of you and are frightened of you. They vanish after 1 minute.</p>","img":"icons/svg/d20-black.svg","weight":1,"range":[37,38],"drawn":false,"documentCollection":"","documentId":null,"_id":"e17uCdKQ6PEaj5It"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">You regain[[2d10]] ([[/r 2d10]])  hit points.</p>","img":"icons/svg/d20-black.svg","weight":1,"range":[39,40],"drawn":false,"documentCollection":"","documentId":null,"_id":"KPj5bGR5mR7ZAO1v"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">You turn into a potted plant until the start of your next turn. While a plant, you are incapacitated and have vulnerability to all damage. If you drop to 0 hit points, your pot breaks, and your form reverts.</p>","img":"icons/svg/d20-black.svg","weight":1,"range":[41,42],"drawn":false,"documentCollection":"","documentId":null,"_id":"vhZVQcUdqBMml7BE"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">For the next minute, you can teleport up to 20 feet as a bonus action on each of your turns.</p>","img":"icons/svg/d20-black.svg","weight":1,"range":[43,44],"drawn":false,"documentCollection":"","documentId":null,"_id":"QEPLeeEeIeSCP6iX"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">You cast <span class=\"Sans-Serif-Character-Styles_Italic-Sans-Serif\">levitate</span> on yourself.</p>","img":"icons/svg/d20-black.svg","weight":1,"range":[45,46],"drawn":false,"documentCollection":"","documentId":null,"_id":"dCTVUt4vXCod4FuM"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">A unicorn controlled by the DM appears in a space within 5 feet of you, then disappears 1 minute later.</p>","img":"icons/svg/d20-black.svg","weight":1,"range":[47,48],"drawn":false,"documentCollection":"","documentId":null,"_id":"64Wv73aJ2VDMbzrt"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">You can’t speak for the next minute. Whenever you try, pink bubbles float out of your mouth.</p>","img":"icons/svg/d20-black.svg","weight":1,"range":[49,50],"drawn":false,"documentCollection":"","documentId":null,"_id":"daIfpRe4abbUmvt1"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">A spectral shield hovers near you for the next minute, granting you a +2 bonus to AC and immunity to <span class=\"Sans-Serif-Character-Styles_Italic-Sans-Serif\">magic missile.</span></p>","img":"icons/svg/d20-black.svg","weight":1,"range":[51,52],"drawn":false,"documentCollection":"","documentId":null,"_id":"5N76v1gebDndUsgg"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">You are immune to being intoxicated by alcohol for the next[[5d6]] ([[/r 5d6]])  days.</p>","img":"icons/svg/d20-black.svg","weight":1,"range":[53,54],"drawn":false,"documentCollection":"","documentId":null,"_id":"OFAnBdifemvKx1mp"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">Your hair falls out but grows back within 24 hours.</p>","img":"icons/svg/d20-black.svg","weight":1,"range":[55,56],"drawn":false,"documentCollection":"","documentId":null,"_id":"SFyzCyOtFbfQUl39"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">For the next minute, any flammable object you touch that isn’t being worn or carried by another creature bursts into flame.</p>","img":"icons/svg/d20-black.svg","weight":1,"range":[57,58],"drawn":false,"documentCollection":"","documentId":null,"_id":"OSopLZ2cnKJHYPzs"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">You regain your lowest-level expended spell slot.</p>","img":"icons/svg/d20-black.svg","weight":1,"range":[59,60],"drawn":false,"documentCollection":"","documentId":null,"_id":"LunKF6z5sYomnjFq"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">For the next minute, you must shout when you speak.</p>","img":"icons/svg/d20-black.svg","weight":1,"range":[61,62],"drawn":false,"documentCollection":"","documentId":null,"_id":"7hpHbeIcXa73RO2I"},{"flags":{},"type":0,"text":"<p class=\"Table-Styles_Table-Body-for-Table-Cell-Style-\" style=\"text-align:left\">You cast <span class=\"Sans-Serif-Character-Styl